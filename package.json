{
  "name": "api-solo-project",
  "version": "1.0.0",
  "description": "\"Starwars API\"",
  "main": "index.js",
  "repository": "https://github.com/shunTsunasawa/api-solo-project.git",
  "author": "shunTsunasawa <tsunasawa@s.aa@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node --inspect -r ts-node/register -r dotenv/config src/index.ts",
    "start": "ts-node -r dotenv/config dist/index.js",
    "testConnection": "ts-node -r dotenv/config src/scripts/testConnection.ts",
    "build": "tsc -p .",
    "lint": "eslint ./src/**/*.ts --fix",
    "test": "mocha --inspect -r ts-node/register -r dotenv/config src/tests/index.ts",
    "orm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "makeMigrations": "yarn orm migration:generate",
    "migrate": "yarn orm migration:run",
    "rollback": "echo 'FIXME!'",
    "seeder": "ts-node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js",
    "seed": "yarn seeder --configName src/ormconfig.ts seed",
    "pre-commit": "yarn lint"
  },
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-http": "^4.2.0",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/pg": "^7.14.3",
    "@types/uuid": "^7.0.2",
    "bcrypt": "4.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.pick": "^4.4.0",
    "mocha": "^7.1.1",
    "pg": "^7.18.2",
    "prettier": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
